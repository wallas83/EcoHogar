<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="grupo11.ecohogar.DatosVivienda">
 <ScrollView
     android:layout_width="match_parent"
     android:layout_height="match_parent"
     android:layout_gravity="center">
     <LinearLayout
         android:layout_width="wrap_content"
         android:layout_height="wrap_content"
         android:orientation="vertical">
         <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_marginLeft="30dp"
             android:layout_marginTop="70dp"
             android:layout_marginRight="30dp"
             android:layout_gravity="center">
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="Datos Vivienda"
                 android:textStyle="bold"
                 android:textSize="30dp" />

         </LinearLayout>
         <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_marginLeft="30dp"
             android:layout_marginTop="30dp"
             android:layout_marginRight="30dp"
             android:layout_marginBottom="10dp">
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="Nro Vivienda: "
                 android:textStyle="bold"
                 android:textSize="20dp"/>
             <TextView
                 android:id="@+id/txtNrovivienda"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="nro viv usuario"
                 android:textSize="20dp"/>
         </LinearLayout>
         <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_marginRight="30dp"
             android:layout_marginLeft="30dp"
             android:layout_marginBottom="10dp">
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="Nro Habitantes: "
                 android:textStyle="bold"
                 android:textSize="20dp"/>
             <TextView
                 android:id="@+id/txtNrohabitantes"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="nro habitantes"
                 android:textSize="20dp"/>
         </LinearLayout>
         <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_marginLeft="30dp"
             android:layout_marginRight="30dp"
             android:layout_marginBottom="10dp">
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="Zona: "
                 android:textStyle="bold"
                 android:textSize="20dp"/>
             <TextView
                 android:id="@+id/txtZona"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="zona"
                 android:textSize="20dp"/>
         </LinearLayout>
         <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_marginLeft="30dp"
             android:layout_marginRight="30dp"
             android:layout_marginBottom="10dp">
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="Calle: "
                 android:textStyle="bold"
                 android:textSize="20dp"/>
             <TextView
                 android:id="@+id/txtCalle"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="calle"
                 android:textSize="20dp"/>
         </LinearLayout>
         <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_marginLeft="30dp"
             android:layout_marginRight="30dp"
             android:layout_marginBottom="10dp">
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="Nº Vivienda: "
                 android:textStyle="bold"
                 android:textSize="20dp"/>
             <TextView
                 android:id="@+id/txtNºvivienda"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="# vivienda"
                 android:textSize="20dp"/>
         </LinearLayout>
         <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_marginLeft="30dp"
             android:layout_marginRight="30dp"
             android:layout_marginBottom="10dp">
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="Limite energia: "
                 android:textStyle="bold"
                 android:textSize="20dp"/>
             <TextView
                 android:id="@+id/txtLimiteenergia"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="consumo energia"
                 android:textSize="20dp"/>
         </LinearLayout>
         <LinearLayout
             android:layout_width="wrap_content"
             android:layout_height="wrap_content"
             android:orientation="horizontal"
             android:layout_marginLeft="30dp"
             android:layout_marginRight="30dp">
             <TextView
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="Limite agua: "
                 android:textStyle="bold"
                 android:textSize="20dp"/>
             <TextView
                 android:id="@+id/txtLimiteagua"
                 android:layout_width="wrap_content"
                 android:layout_height="wrap_content"
                 android:text="consumo agua"
                 android:textSize="20dp"/>
         </LinearLayout>
     </LinearLayout>
 </ScrollView>
</FrameLayout>
